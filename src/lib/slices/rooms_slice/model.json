          }
        },
        "feature_4": {
          "type": "Text",
          "config": {
            "label": "Feature 4",
            "placeholder": "Frühstück inklusive"
          }
        },
        "button_text": {
          "type": "Text",
          "config": {
            "label": "Button Text",
            "placeholder": "Jetzt Buchen"
          }
        },
        "booking_url": {
          "type": "Text",
          "config": {
